import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner (System.in);
        int a,b,i,sum=0;
        a=s.nextInt();
        b=s.nextInt();
    
        for(i=a;i<=b;i++){
            sum +=i;
        }
        System.out.printf("%d",sum);
    }
}
