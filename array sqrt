import java.util.*;
class Test{
public static void main(String[] args)
{
Scanner s=new Scanner(System.in);
int n=s.nextInt();
int a[]=new int[n];
int b[]=new int[n];
for(int i=0;i<n;i++)
{
a[i]=s.nextInt();
}
for(int i=0;i<n;i++)
{
int k=Math.abs(a[i]*a[i]);
b[i]=k;
}
Arrays.sort(b);
for(int i=0;i<n;i++)
{
System.out.print(b[i]+" ");
}
}
}
