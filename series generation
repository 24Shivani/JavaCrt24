import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int c=7,n,i;
        
        n=s.nextInt();
     System.out.printf("%d ",c);
        for( i=1;i<n;i++)
        if(i%2 != 0){
         System.out.printf("%d ",c-=2);   
        }
        else{
          System.out.printf("%d ",c+=3);  
        }
    }

}
