import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int n,sum=1;
        while(true){
            n=s.nextInt();
        sum=sum+n;
        if(n==-1) 
            break;
    }
        
        
    System.out.printf("%d\n",sum);
}
}
